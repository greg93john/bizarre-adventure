<body>
  <div class="page-container">

    <section class="fog">
      <div class="absolute-bg"></div>
      <div class="fog-container">
        <div class="fog-img fog-img-first"></div>
        <div class="fog-img fog-img-second"></div>
      </div>

      <div class="container-fluid content-wrap h-100">
        <div class="row h-100 align-items-center">
          <div class="col d-flex justify-content-center">
            <a href="/take-a-stand">
              <div class="text-center text-light">
                <img src="/images/Speedwagon_Academy_Logo.png">
                <p class="take-a-stand-font pt-3">Take A Stand</p>
              </div>
            </a>
          </div>
          <div class="col d-flex justify-content-center">
            <a href="/stand-against">
              <div class="text-center text-light">
                <img src="/images/Stand_Against_Icon.png">
                <p class="take-a-stand-font pt-3">Stand Against</p>
              </div>
            </a>
          </div>
        </div>
      </div>
    </section>

    <footer class="text-center text-light bg-dark d-flex justify-content-center align-items-center">
      <p class="m-0">Copyright 2020-<%=new Date().getFullYear()%></p>
    </footer>

  </div>
  <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
